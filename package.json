{
  "name": "supdrive",
  "version": "2.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "ng serve --ssl",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer --no-progress",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "docker build -t skynewz/supdrive-front .",
    "docker:run": "docker run -it --rm -p 80:80 skynewz/supdrive-front",
    "electron": "electron .",
    "electron:build": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-packager . --platform=linux --arch=x64 --out=electron",
    "electron:macos": "npm run build:prod && electron-packager . --platform=darwin --arch=x64 --out=electron",
    "electron:windows": "npm run build:prod && electron-packager . --platform=win32 --arch=x64 --out=electron"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:SkYNewZ/supdrive-front.git"
  },
  "author": {
    "email": "quentin@lemairepro.fr",
    "name": "SkYNewZ",
    "url": "https://www.lemairepro.fr"
  },
  "icon": "https://s3.eu-west-3.amazonaws.com/supdrive/icon.png",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animation": "^4.0.0-beta.8",
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@auth0/angular-jwt": "^2.0.0",
    "angularx-social-login": "^1.1.8",
    "class-transformer": "^0.1.9",
    "core-js": "^2.5.4",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ngx-file-drop": "^4.0.4",
    "ngx-toastr": "^8.7.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "ts-md5": "^1.2.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "electron": "^2.0.5",
    "electron-packager": "^12.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
